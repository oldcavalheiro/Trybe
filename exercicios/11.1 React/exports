// CommonJS

const require = require('require'); // 1

module.exports = exportar; // 2

module.exports = {
export1,
export2,
}; // 3

// ES6

import exportar { nome_do_será_exportado } from './path'; // 1

export default exportar // 2

export nome_do_será_exportado; // 3

// 1 - importação
// 2 - export do valor default ou padrão
// 3 - export de valores que não são o padrão ou múltiplos valores 
